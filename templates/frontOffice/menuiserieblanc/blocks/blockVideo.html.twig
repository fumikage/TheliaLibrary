{% if data.videoId is defined %}
    <iframe
      class="tb-{{type.id}} w-full max-w-[1080px] mx-auto aspect-video"
      src="https://www.youtube.com/embed/{{data.videoId}}"
      allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; scripts; same-origin"
    ></iframe>
{% endif %}
